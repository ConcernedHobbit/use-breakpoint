{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es2020"
    ],
    "module": "es2020",
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2020"
  },
  "include": [
    "./.storybook/*.js",
    "./**/*.js",
    "./**/*.ts",
    "./**/*.tsx"
  ]
}